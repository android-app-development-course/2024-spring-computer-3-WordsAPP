<?xml version="1.0" encoding="utf-8"?>
<!--Button样式，本项目中并未使用到该样式-->
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <gradient
        android:startColor="#00000000"
        android:endColor="#00000000"
        android:angle="90" /><!--背景颜色渐变 angle为渐变角度-->
    <solid
        android:color="#00ffffff" /><!-- 背景填充颜色 -->
    <stroke
        android:width="1dp"
        android:color="#01DFA5" /><!-- 描边，边框宽度、颜色 -->
    <corners
        android:radius="10dp" /><!-- 边角圆弧的半径 -->
    <padding
        android:left="0dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="0dp" /><!-- 四周留出来的空白 -->
</shape>